#!/usr/bin/env python3
#zadanie16
arc=["white","black","red","blue","green","yellow"]
k=input("Wpisz wybrany kolor po angielsku ")
if k in arc:
    poz=arc.index(k)
    print("podany kolor znaleziono na pozycji "+str(poz))
else:
    print("podany kolor nie znaleziono na li≈õcie")
